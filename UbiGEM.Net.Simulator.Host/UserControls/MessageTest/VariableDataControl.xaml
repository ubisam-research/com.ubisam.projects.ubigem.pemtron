<UserControl x:Class="UbiGEM.Net.Simulator.Host.UserControls.MessageTest.VariableDataControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Loaded="UserControl_Loaded">
    <Grid>
        <GroupBox Header="Variable Data Message Test" VerticalAlignment="Center" MouseUp="GroupBox_MouseUp">
            <Expander Name="expander" IsExpanded="False" Margin="0, -49, -2, 0" FlowDirection="RightToLeft" Padding="0 10 0 0">
                <WrapPanel Orientation="Horizontal" Margin="0" FlowDirection="LeftToRight">
                    <WrapPanel.Resources>
                        <Style TargetType="Grid">
                            <Setter Property="Margin" Value="5, 7, 5, 0"/>
                        </Style>
                    </WrapPanel.Resources>
                    <Grid Width="150" Height="32">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="5" />
                            <ColumnDefinition Width="50" />
                        </Grid.ColumnDefinitions>
                        <Button Grid.Column="0" Content="S1F3" ToolTip="Selected Equipment Status Request" Click="btnS1F3_Click" />
                        <Button Grid.Column="2" Content="Edit" Click="btnS1F3Edit_Click" />
                    </Grid>
                    <Grid Width="150" Height="32">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="5" />
                            <ColumnDefinition Width="50" />
                        </Grid.ColumnDefinitions>
                        <Button Grid.Column="0" Content="S1F11" ToolTip="Status Variable Namelist Request" Click="btnS1F11_Click" />
                        <Button Grid.Column="2" Content="Edit" Click="btnS1F11Edit_Click" />
                    </Grid>
                    <Grid Width="150" Height="32">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="5" />
                            <ColumnDefinition Width="50" />
                        </Grid.ColumnDefinitions>
                        <Button Grid.Column="0" Content="S1F21" ToolTip="Data Variable Namelist Request" Click="btnS1F21_Click" />
                        <Button Grid.Column="2" Content="Edit" Click="btnS1F21Edit_Click" />
                    </Grid>
                    <Grid Width="150" Height="32">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="5" />
                            <ColumnDefinition Width="50" />
                        </Grid.ColumnDefinitions>
                        <Button Grid.Column="0" Content="S2F13" ToolTip="Equipment Constant Request" Click="btnS2F13_Click" />
                        <Button Grid.Column="2" Content="Edit" Click="btnS2F13Edit_Click" />
                    </Grid>
                    <Grid Width="150" Height="32">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="5" />
                            <ColumnDefinition Width="50" />
                        </Grid.ColumnDefinitions>
                        <Button Grid.Column="0" Content="S2F15" ToolTip="New Equipment Constant Send" Click="btnS2F15_Click" />
                        <Button Grid.Column="2" Content="Edit" Click="btnS2F15Edit_Click" />
                    </Grid>
                    <Grid Width="150" Height="32">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="5" />
                            <ColumnDefinition Width="50" />
                        </Grid.ColumnDefinitions>
                        <Button Grid.Column="0" Content="S2F29" ToolTip="Equipment Constant Namelist Request" Click="btnS2F29_Click" />
                        <Button Grid.Column="2" Content="Edit" Click="btnS2F29Edit_Click" />
                    </Grid>
                </WrapPanel>
            </Expander>
        </GroupBox>
    </Grid>
</UserControl>