<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:UbiSam.Common.Themes.WPF.Themes">

    <Style x:Key="UbiSamThemeTreeViewItem" TargetType="{x:Type TreeViewItem}">
        <Setter Property="Foreground" Value="{DynamicResource UbiSamThemeForeground}"/>
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="FontFamily" Value="돋움"/>
        <Setter Property="FontWeight" Value="Normal"/>
    </Style>
</ResourceDictionary>